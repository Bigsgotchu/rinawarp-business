#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node-ios-device@4.0.3/node_modules/node-ios-device/bin/node_modules:/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node-ios-device@4.0.3/node_modules/node-ios-device/node_modules:/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node-ios-device@4.0.3/node_modules:/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node-ios-device@4.0.3/node_modules/node-ios-device/bin/node_modules:/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node-ios-device@4.0.3/node_modules/node-ios-device/node_modules:/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node-ios-device@4.0.3/node_modules:/home/karina/Documents/rinawarp-business/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-ios-device/bin/node-ios-device" "$@"
else
  exec node  "$basedir/../node-ios-device/bin/node-ios-device" "$@"
fi
