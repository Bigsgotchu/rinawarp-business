{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "node",
      "vite/client"
    ],
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "worker/src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "out",
    "dist",
    "dist-terminal-pro",
    "build-output",
    "release",
    "coverage",
    ".vite",
    "legacy"
  ]
}