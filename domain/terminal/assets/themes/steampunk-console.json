{
  "name": "Steampunk Console",
  "description": "Brass and copper steampunk interface with mechanical aesthetics",
  "version": "1.0.0",
  "author": "RinaWarp Technologies",
  "colors": {
    "background": "#1a0f0a",
    "foreground": "#d4af37",
    "cursor": "#ff6b35",
    "selection": "#8b4513",
    "black": "#0a0500",
    "red": "#8b0000",
    "green": "#228b22",
    "yellow": "#daa520",
    "blue": "#4169e1",
    "magenta": "#8b008b",
    "cyan": "#20b2aa",
    "white": "#f5deb3",
    "brightBlack": "#2f1b14",
    "brightRed": "#cd5c5c",
    "brightGreen": "#90ee90",
    "brightYellow": "#ffd700",
    "brightBlue": "#87ceeb",
    "brightMagenta": "#da70d6",
    "brightCyan": "#40e0d0",
    "brightWhite": "#ffffff"
  },
  "ui": {
    "accentColor": "#d4af37",
    "warningColor": "#ff6b35",
    "successColor": "#228b22",
    "errorColor": "#8b0000",
    "infoColor": "#4169e1",
    "borderColor": "#8b4513",
    "shadowColor": "rgba(212, 175, 55, 0.2)"
  },
  "fonts": {
    "primary": "Courier New, 'Monaco', monospace",
    "secondary": "Cinzel, 'Times New Roman', serif",
    "size": 14,
    "lineHeight": 1.3
  },
  "effects": {
    "brassTexture": true,
    "copperAccents": true,
    "gearAnimations": true,
    "steamEffect": true,
    "vintageGlow": true,
    "mechanicalSounds": true
  },
  "animations": {
    "gearSpeed": 0.5,
    "steamIntensity": 0.4,
    "typingEffect": "mechanical",
    "cursorBlink": "slow"
  },
  "sounds": {
    "keyClick": "typewriter-clack",
    "enterSound": "steam-whistle",
    "errorSound": "gear-grind",
    "successSound": "bell-chime"
  },
  "layout": {
    "borderWidth": 3,
    "cornerRadius": 4,
    "padding": 16,
    "gearSize": 40
  }
}
