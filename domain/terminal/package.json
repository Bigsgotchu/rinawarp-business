{
  "name": "terminal",
  "version": "1.0.0",
  "description": "RinaWarp Terminal Pro â€” Cross-platform AI terminal assistant.",
  "author": "Karina <rinawarptechnologies25@gmail.com>",
  "homepage": "https://rinawarptech.com",
  "main": "start.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:electron\"",
    "dev:api": "nodemon src/domain/terminal/server.js --watch src/domain/terminal",
    "dev:electron": "electron start.cjs",
    "dev:vite": "vite",
    "build": "vite build --mode production",
    "build:optimized": "vite build --mode production --outDir dist-optimized",
    "postinstall": "node scripts/postinstall.js",
    "make": "electron-builder",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stripe:e2e": "node -r dotenv/config stripe-test-endtoend-safe.js",
    "verify:stripe": "node -r dotenv/config verify-stripe-key.js",
    "dev:pm2": "npx pm2 start Procfile.dev --name rinawarp-dev",
    "dev:foreman": "npx nf start -f Procfile.dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.21.0",
    "@tanstack/react-query": "^5.62.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^8.1.0",
    "groq-sdk": "^0.7.0",
    "helmet": "^7.1.0",
    "i18next": "^23.15.2",
    "jsonwebtoken": "^9.0.2",
    "keytar": "^7.9.0",
    "ollama": "^0.5.18",
    "openai": "^4.68.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.2",
    "stripe": "^14.22.0",
    "vite": "^7.1.11",
    "web-vitals": "^4.2.4",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.7",
    "@tailwindcss/postcss": "^4.1.15",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "aws-sdk": "^2.1692.0",
    "babel-jest": "^29.7.0",
    "electron": "^38.4.0",
    "electron-builder": "^24.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "foreman": "^3.0.1",
    "electron-reload": "^1.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "nodemailer": "^7.0.10",
    "nodemon": "^3.1.7",
    "pm2": "^6.0.13",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.1.2"
  },
  "build": {
    "productName": "Rinawarp Terminal Pro",
    "appId": "com.rinawarp.terminalpro",
    "directories": {
      "output": "release",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "assets/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.mjs"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development",
      "icon": "assets/icon.png"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-${version}-win-${arch}.${ext}"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns"
    }
  },
  "keywords": [],
  "license": "ISC"
}
